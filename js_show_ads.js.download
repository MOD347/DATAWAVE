ad_format='';ad_border='';ad_type='';ad_channel='';function show_ads(id){var xhr=new XMLHttpRequest();var method='GET';var data=new FormData();xhr.open(method,'/ads&channel='+ad_channel+'&format='+ad_format+'&type='+ad_type+'&border='+ad_border+'&ajax=1',true);xhr.onreadystatechange=function(e){if(xhr.readyState==4){if(xhr.status==200){if(xhr.responseText){try{var json=JSON.parse(xhr.responseText);if(json.error){}else{var div=document.getElementById(id);div.innerHTML=json.adv;}}catch(e){}}}}};xhr.send(data);}